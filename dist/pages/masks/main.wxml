<import src="../../wech/components/mask/mask.wxml" />
<import src="../../wech/components/alert/alert.wxml" />

<view class="root">
	<view class="block">
		<view class="button" bindtap="showMask">展示mask</view>
	</view>
	<template is="wech_mask" data="{{ ...mask }}"/>

	<view class="block">
		<view class="button" bindtap="showAlert">展示alert</view>
	</view>
	<template is="wech_alert" data="{{ ...alert }}"/>

</view>