.wech_mask{position:fixed;pointer-events:none;top:0;height:100%;left:0;width:100%;opacity:0;background:black;transition:opacity .2s linear}.wech_mask-visible{pointer-events:initial;z-index:10;display:block;opacity:.5}.wech_alert{position:fixed;pointer-events:none;top:50%;left:50%;width:300rpx;margin:-60rpx 0 0 -170rpx;background:black;color:white;text-align:center;line-height:38rpx;font-size:32rpx;padding:20rpx;border-radius:8rpx}
